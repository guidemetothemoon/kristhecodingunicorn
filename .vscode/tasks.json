{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "git-branch-cleanup",
            "type": "shell",
            "command": "git fetch -p && git branch -vv | awk '/: gone]/{print $1}' | xargs git branch -d",
            "problemMatcher": [],
            "presentation": {
                "reveal": "always"
            },
            "runOptions": {
                "runOn": "folderOpen"
            }
        }
    ]
}